{
  "name": "@wyrecc/worker",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:code": "tsc",
    "build": "run-s build:clean build:code",
    "dev": "nodemon src/entry.ts",
    "dev:debug": "nodemon --inspect src/entry.ts",
    "format": "prettier --write '**/*.{js,ts}'",
    "lint:es": "next lint --fix",
    "lint:ts": "tsc --pretty --noEmit",
    "prod": "run-s build && node dist/entry.js",
    "start": "node dist/entry.js",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "@wyrecc/env": "^0.1.0",
    "@wyrecc/maplerad": "^0.1.0",
    "bull": "^4.10.2",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "ioredis": "^5.2.5",
    "lodash": "^4.17.20",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "4.17.8",
    "@types/jest": "29.2.6",
    "@wyrecc/tsconfig": "0.1.0",
    "@types/lodash": "^4.14.161",
    "@types/supertest": "2.0.10",
    "jest": "29.3.1",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "ts-jest": "29.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  }
}
