{
  "name": "wyre",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "npx prettier --write src",
    "postinstall": "yarn prisma generate && yarn db-sync",
    "db-deploy": "yarn prisma migrate deploy",
    "db-generate": "yarn prisma generate",
    "db-sync": "yarn prisma migrate dev",
    "db-migrate": "yarn prisma migrate dev --name",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "db-reset": "run-s db-nuke db-setup",
    "db-seed": "yarn prisma db seed",
    "db-setup": "run-s db-up db-deploy db-seed",
    "db-studio": "yarn prisma studio",
    "db-up": "docker-compose up -d",
    "dx": "yarn db-setup",
    "lint": "next lint",
    "lint:ts": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest -i"
  },
  "dependencies": {
    "@ajna/pagination": "^1.4.19",
    "@chakra-ui/react": "^2.3.7",
    "@chakra-ui/theme-tools": "^2.0.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.10.0",
    "@trpc/client": "10.0.0-rc.4",
    "@trpc/next": "10.0.0-rc.4",
    "@trpc/react-query": "10.0.0-rc.4",
    "@trpc/server": "10.0.0-rc.4",
    "framer-motion": "^7.6.5",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "nodemailer": "^6.8.0",
    "react": "18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.6.0",
    "react-select": "^5.6.0",
    "superjson": "1.9.1",
    "trpc-playground": "1.0.0-next.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.2.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.2",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.5.0",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.8.0"
  }
}
